    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.400.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.0",
    "aws-cdk": "^2.120.0",
    "esbuild": "^0.19.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
